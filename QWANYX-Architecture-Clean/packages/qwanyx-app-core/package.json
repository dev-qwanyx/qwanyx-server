{
  "name": "@qwanyx/app-core",
  "version": "0.1.0",
  "description": "Core orchestrator package for QWANYX applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/styles.css": "./dist/styles.css",
    "./styles": "./dist/styles.css"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@qwanyx/ui": "file:../qwanyx-ui",
    "@qwanyx/auth": "file:../qwanyx-auth",
    "@qwanyx/dashboard-v2": "file:../qwanyx-dashboard-v2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  }
}